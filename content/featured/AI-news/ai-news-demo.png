<svg width="600" height="400" viewBox="0 0 600 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background Gradient -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#0a192f;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#112240;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="cardGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#233554;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1a1a3a;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="600" height="400" fill="url(#bgGradient)"/>
  
  <!-- Header Bar -->
  <rect x="0" y="0" width="600" height="50" fill="#020c1b"/>
  <circle cx="25" cy="25" r="8" fill="#64ffda"/>
  <circle cx="45" cy="25" r="8" fill="#ff6b6b"/>
  <circle cx="65" cy="25" r="8" fill="#4ecdc4"/>
  <text x="300" y="32" font-family="Arial, sans-serif" font-size="18" font-weight="bold" text-anchor="middle" fill="#e6f1ff">AI News Hub</text>
  
  <!-- Sidebar -->
  <rect x="0" y="50" width="150" height="350" fill="#020c1b"/>
  <rect x="10" y="70" width="130" height="30" fill="#64ffda" rx="15"/>
  <text x="75" y="90" font-family="Arial, sans-serif" font-size="12" font-weight="bold" text-anchor="middle" fill="#0a192f">Dashboard</text>
  
  <rect x="10" y="110" width="130" height="25" fill="transparent" stroke="#64ffda" stroke-width="1" rx="12"/>
  <text x="75" y="127" font-family="Arial, sans-serif" font-size="11" text-anchor="middle" fill="#64ffda">Tech News</text>
  
  <rect x="10" y="145" width="130" height="25" fill="transparent" stroke="#64ffda" stroke-width="1" rx="12"/>
  <text x="75" y="162" font-family="Arial, sans-serif" font-size="11" text-anchor="middle" fill="#64ffda">AI Updates</text>
  
  <rect x="10" y="180" width="130" height="25" fill="transparent" stroke="#64ffda" stroke-width="1" rx="12"/>
  <text x="75" y="197" font-family="Arial, sans-serif" font-size="11" text-anchor="middle" fill="#64ffda">Science</text>
  
  <!-- Main Content Area -->
  <rect x="160" y="70" width="430" height="320" fill="url(#cardGradient)" rx="8"/>
  
  <!-- Search Bar -->
  <rect x="180" y="90" width="350" height="35" fill="#020c1b" rx="17"/>
  <text x="200" y="110" font-family="Arial, sans-serif" font-size="12" fill="#8892b0">Search AI news...</text>
  <circle cx="510" cy="107" r="12" fill="#64ffda"/>
  <text x="510" y="112" font-family="Arial, sans-serif" font-size="10" text-anchor="middle" fill="#0a192f">üîç</text>
  
  <!-- News Cards -->
  <!-- Card 1 -->
  <rect x="180" y="140" width="350" height="60" fill="#020c1b" rx="6"/>
  <rect x="190" y="150" width="50" height="40" fill="#64ffda" rx="4"/>
  <text x="215" y="175" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#0a192f">AI</text>
  <text x="250" y="165" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#e6f1ff">OpenAI Releases GPT-5</text>
  <text x="250" y="180" font-family="Arial, sans-serif" font-size="11" fill="#8892b0">Revolutionary AI model with enhanced capabilities...</text>
  <text x="250" y="195" font-family="Arial, sans-serif" font-size="10" fill="#64ffda">2 hours ago</text>
  
  <!-- Card 2 -->
  <rect x="180" y="210" width="350" height="60" fill="#020c1b" rx="6"/>
  <rect x="190" y="220" width="50" height="40" fill="#ff6b6b" rx="4"/>
  <text x="215" y="245" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#ffffff">ML</text>
  <text x="250" y="235" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#e6f1ff">Machine Learning in Healthcare</text>
  <text x="250" y="250" font-family="Arial, sans-serif" font-size="11" fill="#8892b0">New algorithms improve patient diagnosis...</text>
  <text x="250" y="265" font-family="Arial, sans-serif" font-size="10" fill="#64ffda">4 hours ago</text>
  
  <!-- Card 3 -->
  <rect x="180" y="280" width="350" height="60" fill="#020c1b" rx="6"/>
  <rect x="190" y="290" width="50" height="40" fill="#4ecdc4" rx="4"/>
  <text x="215" y="315" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#0a192f">TECH</text>
  <text x="250" y="305" font-family="Arial, sans-serif" font-size="14" font-weight="bold" fill="#e6f1ff">Quantum Computing Breakthrough</text>
  <text x="250" y="320" font-family="Arial, sans-serif" font-size="11" fill="#8892b0">Scientists achieve quantum supremacy...</text>
  <text x="250" y="335" font-family="Arial, sans-serif" font-size="10" fill="#64ffda">6 hours ago</text>
  
  <!-- AI Processing Indicator -->
  <rect x="500" y="350" width="80" height="20" fill="#64ffda" rx="10"/>
  <text x="540" y="363" font-family="Arial, sans-serif" font-size="10" font-weight="bold" text-anchor="middle" fill="#0a192f">AI Processing</text>
  
  <!-- Floating AI Icon -->
  <circle cx="550" cy="50" r="15" fill="#64ffda" opacity="0.8"/>
  <text x="550" y="56" font-family="Arial, sans-serif" font-size="12" font-weight="bold" text-anchor="middle" fill="#0a192f">AI</text>
</svg>
